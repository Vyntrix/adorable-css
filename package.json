{
  "name": "adorable-css-docs",
  "version": "0.0.1",
  "homepage": "https://developer-1px.github.io/adorable-css",
  "scripts": {
    "dev": "vite dev",
    "build": "rm -rf build && NODE_ENV=production vite build && touch ./build/.nojekyll",
    "deploy": "npm run build && gh-pages -d build -t true",
    "rebuild": "git commit --allow-empty -m \"Trigger rebuild\"",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@types/clean-css": "^4.2.5",
    "@types/glob": "^7.2.0",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "gh-pages": "^4.0.0",
    "mdsvex": "^0.10.6",
    "micromatch": "^4.0.5",
    "monaco-editor": "^0.34.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "adorable-css": "file:./packages/vite/dist",
    "vite": "^3.0.7"
  },
  "type": "module",
  "dependencies": {
    "cac": "^6.7.12",
    "chokidar": "^3.5.3",
    "clean-css": "^5.3.1",
    "glob": "^8.0.3"
  }
}
