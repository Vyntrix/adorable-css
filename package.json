{
  "type": "module",
  "name": "adorable-css-docs",
  "version": "1.3.0",
  "homepage": "https://developer-1px.github.io/adorable-css",
  "scripts": {
    "dev": "vite dev",
    "build": "rm -rf build && NODE_ENV=production vite build && touch ./build/.nojekyll",
    "deploy": "npm run build && gh-pages -d build -t true",
    "rebuild": "git commit --allow-empty -m \"Trigger rebuild\"",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@types/clean-css": "^4.2.6",
    "@types/glob": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "cac": "^6.7.14",
    "clean-css": "^5.3.2",
    "eslint": "^8.47.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "gh-pages": "^6.0.0",
    "glob-to-regexp": "^0.4.1",
    "mdsvex": "^0.11.0",
    "micromatch": "^4.0.5",
    "monaco-editor": "^0.41.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "chokidar": "^3.5.3",
    "glob": "^10.3.3"
  }
}
